{
  "manifest_version": 2,
  "name": "ClickTabUniq",
  "version": "1.0.1",
  "description": "Close duplicated tabs from right click menu on tabs.\n\nYou can close duplicated tabs based on URL or title.\nPinned tabs are not closed.",
  "applications": {
    "gecko": {
      "id": "{e3e32590-2c56-405d-9f0f-9dc571a87d67}",
      "strict_min_version": "53.0"
    }
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "storage"
  ],
  "background": {
    "scripts": [
      "uniq.js"
    ]
  },
  "default_locale": "en",
  "options_ui": {
    "page": "options.html"
  }
}
